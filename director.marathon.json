{
   "id":"/riak-director",
   "cmd":"./riak_mesos_director/director_linux_amd64",
   "cpus":0.5,
   "mem":1024.0,
   "ports":[0,0,0,0],
   "instances":1,
   "env":{
      "USE_SUPER_CHROOT": "true",
      "FRAMEWORK_HOST":"localhost",
      "FRAMEWORK_PORT":"31559",
      "DIRECTOR_ZK":"localhost:2181",
      "DIRECTOR_FRAMEWORK":"riak",
      "DIRECTOR_CLUSTER":"mycluster"
   },
   "uris":[
      "http://riak-tools.s3.amazonaws.com/riak-mesos/coreos/riak_mesos_director_linux_amd64_0.1.1.tar.gz"
   ],
   "healthChecks":[
      {
         "protocol":"HTTP",
         "path":"/health",
         "gracePeriodSeconds":3,
         "intervalSeconds":10,
         "portIndex":2,
         "timeoutSeconds":10,
         "maxConsecutiveFailures":3
      }
   ]
}
