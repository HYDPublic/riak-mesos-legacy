{
  "id": "/riak1",
  "instances": 1,
  "cpus": 0.5,
  "mem": 512,
  "ports": [0,0],
  "uris": [
      "http://33.33.33.1:8000/_build/riak_mesos_linux_amd64_0.1.0.tar.gz"
  ],
  "env": {},
  "args": [
      "riak_mesos_framework/framework_linux_amd64",
      "-master=zk://33.33.33.2:2181/mesos",
      "-zk=33.33.33.2",
      "-ip=ubuntu.local",
      "-hostname=ubuntu.local",
      "-log=/riak-mesos/test.log",
      "-user=vagrant",
      "-name=riakMesosFramework",
      "-role=*"],
  "healthChecks": [
    {
      "path": "/healthcheck",
      "portIndex": 0,
      "protocol": "HTTP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 5,
      "ignoreHttp1xx": false
    }
  ]
}
