{
  "id": "/riak",
  "instances": 1,
  "cpus": 0.5,
  "mem": 512,
  "ports": [0,0],
  "uris": [],
  "env": {},
  "args": ["/vagrant/bin/framework_linux_amd64", "-master=zk://33.33.33.2:2181/mesos", "-zk=33.33.33.2", "-ip=ubuntu.local", "-hostname=ubuntu.local", "-id=riak-mesos-go", "-log=/vagrant/test.log", "-user=vagrant"],
  "healthChecks": [
    {
      "path": "/healthcheck",
      "portIndex": 0,
      "protocol": "HTTP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 5,
      "ignoreHttp1xx": false
    }
  ]
}
