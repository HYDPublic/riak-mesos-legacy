{
  "id": "riak",
  "instances": 1,
  "cpus": 0.5,
  "mem": 2048,
  "ports": [0, 0],
  "uris": ["http://riak-tools.s3.amazonaws.com/riak-mesos/coreos/riak_mesos_linux_amd64_0.1.1.tar.gz"],
  "env": {},
  "cmd": "riak_mesos_framework/framework_linux_amd64 -master=zk://localhost:2181/mesos -zk=localhost:2181 -name=riak -user=root",
  "healthChecks": [
    {
      "path": "/healthcheck",
      "portIndex": 0,
      "protocol": "HTTP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 5,
      "ignoreHttp1xx": false
    }]
}
